{
    "name": "web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NEXT_PUBLIC_ENVIRONMENT=development next dev -p 3003",
        "dev:prod": "cross-env NEXT_PUBLIC_ENVIRONMENT=production next dev -p 3003",
        "build": "npm run i18n && next build",
        "start": "next start",
        "lint": "eslint .",
        "extract:i18n": "formatjs extract '{pages,components,sections}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
        "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
        "i18n": "npm run extract:i18n && npm run compile:i18n"
    },
    "dependencies": {
        "@chakra-ui/next-js": "^2.1.3",
        "@chakra-ui/react": "^2.6.1",
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@types/node": "20.1.3",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "autoprefixer": "10.4.14",
        "axios": "^1.4.0",
        "chatui": "*",
        "socketclass": "*",
        "framer-motion": "^10.12.10",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "next": "13.4.2",
        "postcss": "8.4.23",
        "react": "18.2.0",
        "react-cookie": "^4.1.1",
        "react-dom": "18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-intl": "^6.4.2",
        "socket.io-client": "4.5.1",
        "tailwindcss": "3.3.2",
        "typescript": "5.0.4",
        "utf-8-validate": "^6.0.3"
    },
    "devDependencies": {
        "@formatjs/cli": "^6.1.1",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "babel-plugin-formatjs": "^10.5.1",
        "eslint": "^8.40.0",
        "eslint-config-next": "^13.4.2",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-eslint-plugin": "^5.0.8",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^2.8.8",
        "eslint-config-custom": "*"
    }
}
